<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DonVo2020.DataStructuresAlgorithms.MVC</title>

    <environment names="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
</head>
<body>
    @Html.ActionLink("DLinkedList", "Index", "DLinkedList", null, new { @class = "btn btn-info btn-sm" })
    @Html.ActionLink("Skip List", "Index", "SkipList", null, new { @class = "btn btn-info btn-sm" })
    @Html.ActionLink("Priority Queue", "Index", "PriorityQueue", null, new { @class = "btn btn-info btn-sm" })    

    @Html.ActionLink("Binary Search Tree", "Index", "BinarySearchTree", null, new { @class = "btn btn-info btn-sm" })
    @Html.ActionLink("AVL Tree", "Index", "AVLTree", null, new { @class = "btn btn-info btn-sm" })
    @Html.ActionLink("Red Black Tree", "Index", "RedBlackTree", null, new { @class = "btn btn-info btn-sm" })
    @Html.ActionLink("Clique Graph", "Index", "CliqueGraph", null, new { @class = "btn btn-info btn-sm" })
    @Html.ActionLink("Directed Dense Graph", "Index", "DirectedDenseGraph", null, new { @class = "btn btn-info btn-sm" })  
    @Html.ActionLink("Directed Weighted Dense Graph", "Index", "DirectedWeightedDenseGraph", null, new { @class = "btn btn-info btn-sm" })
    @Html.ActionLink("Undirected Dense Graph", "Index", "UndirectedDenseGraph", null, new { @class = "btn btn-info btn-sm" })
    <hr />
    @Html.ActionLink("Trie Tree", "Index", "TrieTree", null, new { @class = "btn btn-success btn-sm" })
    @Html.ActionLink("Dijkstra Shortest Path", "Index", "DijkstraShortestPaths", null, new { @class = "btn btn-success btn-sm" })
    @Html.ActionLink("Bipartite Coloring", "Index", "BipartiteColoring", null, new { @class = "btn btn-success btn-sm" })
    @Html.ActionLink("Kruskal's Spanning Tree", "Index", "KruskalsAlgorithm", null, new { @class = "btn btn-success btn-sm" })
    @Html.ActionLink("Cycles Detector Graph", "Index", "CyclesDetectorGraph", null, new { @class = "btn btn-success btn-sm" })
    <div class="container body-content">
        @RenderBody()
        @*<hr />
        <footer>
            <p>&copy; 2020 - DonVo2020.DataStructuresAlgorithms.MVC</p>
        </footer>*@
    </div>

    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>
